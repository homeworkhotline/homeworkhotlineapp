<p id="notice"><%= notice %></p>

<h1>Call Logs</h1>

<table>
  <thead>
    <tr>
      <th>Entered by</th>
      <th>Date</th>
      <th>Starttime</th>
      <th>Endtime</th>
      <th>Int</th>
      <th>Math</th>
      <th>Worksheet</th>
      <th>Stoodle</th>
      <th>Image share</th>
      <th>Textbook</th>
      <th>Page</th>
      <th>Chaptitle</th>
      <th>Notes</th>
      <th>Skill</th>
      <th>Startknow</th>
      <th>Endknow</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @call_logs.each do |call_log| %>
      <tr>
        <td><%= call_log.entered_by %></td>
        <td><%= call_log.date %></td>
        <td><%= call_log.starttime %></td>
        <td><%= call_log.endtime %></td>
        <td><%= call_log.int %></td>
        <td><%= call_log.math %></td>
        <td><%= call_log.worksheet %></td>
        <td><%= call_log.stoodle %></td>
        <td><%= call_log.image_share %></td>
        <td><%= call_log.textbook %></td>
        <td><%= call_log.page %></td>
        <td><%= call_log.chaptitle %></td>
        <td><%= call_log.notes %></td>
        <td><%= call_log.skill %></td>
        <td><%= call_log.startknow %></td>
        <td><%= call_log.endknow %></td>
        <td><%= link_to 'Show', call_log %></td>
        <td><%= link_to 'Edit', edit_call_log_path(call_log) %></td>
        <td><%= link_to 'Destroy', call_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Call Log', new_call_log_path %>
