<p id="notice"><%= notice %></p>

<h1>Principals</h1>

<table>
  <thead>
    <tr>
      <th>Ccontactfirst</th>
      <th>Ccontactlast</th>
      <th>Ccontacttitle</th>
      <th>Schoolsystem</th>
      <th>Mailaddress</th>
      <th>Csz</th>
      <th>Phonenumb</th>
      <th>Email</th>
      <th>Textcontactfirst</th>
      <th>Textcontactlast</th>
      <th>Textcontactemail</th>
      <th>Meetone</th>
      <th>Meettwo</th>
      <th>Completedbyfirst</th>
      <th>Completedbylast</th>
      <th>Completedbytitle</th>
      <th>Schooltype</th>
      <th>Peds</th>
      <th>Census</th>
      <th>Ptgs</th>
      <th>Sonicpartner</th>
      <th>Titlei</th>
      <th>Appalachain</th>
      <th>Districtnumb</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @principals.each do |principal| %>
      <tr>
        <td><%= principal.ccontactfirst %></td>
        <td><%= principal.ccontactlast %></td>
        <td><%= principal.ccontacttitle %></td>
        <td><%= principal.schoolsystem %></td>
        <td><%= principal.mailaddress %></td>
        <td><%= principal.csz %></td>
        <td><%= principal.phonenumb %></td>
        <td><%= principal.email %></td>
        <td><%= principal.textcontactfirst %></td>
        <td><%= principal.textcontactlast %></td>
        <td><%= principal.textcontactemail %></td>
        <td><%= principal.meetone %></td>
        <td><%= principal.meettwo %></td>
        <td><%= principal.completedbyfirst %></td>
        <td><%= principal.completedbylast %></td>
        <td><%= principal.completedbytitle %></td>
        <td><%= principal.schooltype %></td>
        <td><%= principal.peds %></td>
        <td><%= principal.census %></td>
        <td><%= principal.ptgs %></td>
        <td><%= principal.sonicpartner %></td>
        <td><%= principal.titlei %></td>
        <td><%= principal.appalachain %></td>
        <td><%= principal.districtnumb %></td>
        <td><%= link_to 'Show', principal %></td>
        <td><%= link_to 'Edit', edit_principal_path(principal) %></td>
        <td><%= link_to 'Destroy', principal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Principal', new_principal_path %>
