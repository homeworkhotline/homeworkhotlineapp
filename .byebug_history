@sessions = CallLog.count
@sessions = CallLog.where.not(duration: nil).count
@sessions = CallLog.where.not(duration: nil).where('date BETWEEN ? AND ?', 28.days.ago, Date.today).count
@sessions = CallLog.where.not(duration: nil).where('date BETWEEN ? AND ?', 28.days.ago, 1.day.ago).count
@sessions = CallLog.where.not(duration: nil).where('date BETWEEN ? AND ?', 28.days.ago.beginning_of_day, 1.day.ago).count
@sessions = CallLog.where.not(duration: nil).where('date BETWEEN ? AND ?', 28.days.ago.beginning_of_day, Date.today).count
@sessions = CallLog.where.not(duration: nil).where('date BETWEEN ? AND ?', 28.days.ago.beginning_of_day, Date.today).size
